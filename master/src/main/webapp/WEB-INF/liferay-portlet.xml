<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 5.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_5_2_0.dtd">


<liferay-portlet-app>
	
	<portlet>
		<portlet-name>semantic-annotation-editor</portlet-name>
		<icon>/img/icon.png</icon>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/js/yui_3.2.0/overlay/assets/overlay-core.css</header-portlet-css>
		<header-portlet-css>/js/yui_3.2.0/overlay/assets/skins/sam/overlay-skin.css</header-portlet-css>
		<!-- Uncomment for Liferay 5, comment for Liferay 6 -->
		<header-portlet-javascript>/js/yui_3.2.0/yui/yui-debug.js</header-portlet-javascript>
		<css-class-wrapper>semantic-annotation-editor</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>